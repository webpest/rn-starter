{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "rules": {
    "linebreak-style": 0,
    "quotes": ["warn", "double"],
    "semi": ["error", "always"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "react/no-array-index-key": 0,
    "no-console": "off",
    "no-unused-vars": "off",
    "react/no-children-prop": "off"
  },
  "plugins": ["react", "jsx-a11y", "import"]
}
